<div class="row">
  <div class="span16">
<%= form_for(resource, :as => resource_name, :url => user_registration_path(resource_name)) do |f| %>
  <p><%= f.text_field :beta_key, :placeholder => "beta key" %></p>
  <p><%= f.email_field :email, :placeholder => "email address" %></p>
  <p><%= f.password_field :password, :placeholder => "password" %></p>
  <p><%= f.password_field :password_confirmation, :placeholder => "password confirmation" %></p>
  <%= f.submit "Register", :class => "btn primary" %>
<% end %>
</div>
</div>